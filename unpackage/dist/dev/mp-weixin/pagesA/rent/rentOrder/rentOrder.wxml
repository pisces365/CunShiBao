<view><view class="rentOrder"><view class="top"><view class="houseDes"><view class="houseDes-left"><image src="{{houseInfo.pictureUrl[0]}}" mode></image></view><view class="houseDes-right"><view class="houseDes-right-top"><text>{{houseInfo.type}}</text><text>|</text><text class="noWarp">{{houseInfo.desc}}</text></view><view class="houseDes-right-center">{{'押一付三 '+(' · '+houseInfo.bedroomAmount+'室 ')+"·"+(houseInfo.area+'m · 南北')+''}}</view><view class="houseDes-right-bottom">{{''+houseInfo.price+'元/月'}}</view></view></view><view class="cross-line"></view><view class="rentOwner"><view class="rentOwner-image"><image src="{{houseInfo.hostRoughVo.avatarUrl}}" mode></image></view><view class="rentOwner-info"><view class="rentOwner-info-left"><view class="rentOwner-info-name"><text class="hostName">{{houseInfo.hostRoughVo.name}}</text><text class="hostDsc">优质房东</text></view><view class="rentOwner-info-address">{{''+houseInfo.location+''}}</view></view></view></view></view><view class="cross-line"></view><view class="date-info"><view class="address">选择看房时间</view><view class="date"><view class="date-title">日期选择</view><view class="datePicker"><picker mode="date" start="{{$root.m0}}" end="{{$root.m1}}" data-event-opts="{{[['change',[['selectDate',['$event']]]]]}}" bindchange="__e"><view class="date-screen">{{dateTime==''?'日期选择':dateTime}}</view></picker></view></view><view class="cross-line"></view><view class="time-select"><block wx:for="{{detailList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['timeSelect',[index]]]]]}}" class="time-detail" style="{{(currentIndex==index?'background-color: #2abffe;':'')}}" bindtap="__e"><text class="time-text" style="{{(currentIndex==index?'color: #fff;':'')}}">{{item.title}}</text></view></block></view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="button" bindtap="__e"><view class="button-content">立即预约</view></view></view>