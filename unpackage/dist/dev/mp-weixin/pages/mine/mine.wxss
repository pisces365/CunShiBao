page {
  background-color: #f2f3f2;
}
.homePage {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.homePage .homePage-title {
  width: 100%;
  height: 250rpx;
  background-image: linear-gradient(#d4e605, #1be554);
  z-index: -1;
  position: fixed;
  top: 0;
  left: 0;
}
.homePage .homePage-PersonalInfo {
  background: #FFFFFF;
  width: 680rpx;
  height: 260rpx;
  margin-top: 100rpx;
  border-radius: 20rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, 0.1);
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-left {
  width: 200rpx;
  margin-left: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-left .PersonalInfo-HeadImage {
  width: 160rpx;
  height: 160rpx;
  border-radius: 100rpx;
  overflow: hidden;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-left .PersonalInfo-HeadImage .PersonalInfo-HeadImage-image {
  width: 100%;
  height: 100%;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-right {
  flex: 1;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-right .right-content {
  width: 90%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-right .right-content .basicInfo {
  display: flex;
  flex-direction: row;
  margin: 0 20rpx 10rpx 0rpx;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-right .right-content .basicInfo .name {
  font-size: 40rpx;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-right .right-content .basicInfo .icon-chuangzuo {
  margin-left: 20rpx;
  color: #96fa8f;
}
.homePage .homePage-PersonalInfo .homePage-PersonalInfo-right .right-content .residentialAddress {
  font-size: 30rpx;
  color: #939493;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.homePage .homePage-RealNameInfo {
  background: #FFFFFF;
  width: 680rpx;
  height: 140rpx;
  margin-top: 40rpx;
  border-radius: 20rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, 0.1);
  position: relative;
}
.homePage .homePage-RealNameInfo .homePage-RealNameInfo-left {
  margin-left: 30rpx;
  font-weight: 500;
}
.homePage .homePage-RealNameInfo .homePage-RealNameInfo-right {
  display: flex;
  flex-direction: row;
  align-items: center;
  position: absolute;
  right: 20rpx;
  color: #A4ADB3;
}
.homePage .homePage-RealNameInfo .homePage-RealNameInfo-right .icon-a-LeftArrow-copy {
  margin-left: 14rpx;
}
.homePage .homePage-work {
  background: #FFFFFF;
  width: 680rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, 0.1);
}
.homePage .homePage-work .cross-line {
  width: 100%;
  height: 1rpx;
  background: rgba(0, 0, 0, 0.1);
}
.homePage .homePage-work .homePage-work-part {
  margin-left: 30rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.homePage .homePage-work .homePage-work-part .homePage-image {
  height: 60rpx;
  width: 60rpx;
}
.homePage .homePage-work .homePage-work-part .homePage-image .homePage-imageSelf {
  width: 100%;
  height: 100%;
}
.homePage .homePage-work .homePage-work-part .homePage-work-part-title {
  font-size: 34rpx;
  font-weight: 500;
  margin-left: 20rpx;
}
.homePage .homePage-work .homePage-work-part .homePage-iconRight {
  position: absolute;
  right: 30rpx;
  font-size: 40rpx;
  color: #b4b5b4;
}
.homePage .homePage-developer {
  width: 620rpx;
  margin-bottom: 40rpx;
}
.homePage .homePage-developer text {
  font-size: 24rpx;
  color: #acadac;
}

