	// https://uniapp.dcloud.io/collocation/pages?id=subpackages
	// 配置preloadRule后，在进入小程序某个页面时，由框架自动预下载可能需要的分包，提升进入后续分包页面时的启动速度
	// packages 进入页面后预下载分包的 root 或 name。__APP__ 表示主包。
	// network  在指定网络下预下载，可选值为：all（不限网络）、wifi（仅wifi下预下载）
	"preloadRule": {
		"pages/welcome/welcome": {
			"network": "all",
			"packages": [
				"__APP__"
			]
		},
		"pages/index/index": {
			"network": "all",
			"packages": [
				"pagesA"
			]
		},
		"pagesB/landContract/landContract": {
			"network": "all",
			"packages": [
				"pagesB"
			]
		}
	}
