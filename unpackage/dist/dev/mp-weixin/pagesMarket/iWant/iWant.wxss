.release-button {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.release-button button {
	width: 100%;
	border-radius: 50rpx;
	margin:0 30rpx;
	font-size: 32rpx;
}
.release-button .r-button1 {
	background-color: #e3e3e3;
	color: white;
}
.release-button .r-button2 {
	background-color: rgba(78,207,104,1);
	color: white;
}
.release-button .r-button1:active {
	background-color: rgba(227,227,227,0.8);
}
