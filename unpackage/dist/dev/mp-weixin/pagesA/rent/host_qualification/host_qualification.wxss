
.titleImg {
	width: 100vw;
	height: 300rpx;
}
.titleImg image {
	width: 100%;
	height: 100%;
}
.content {
	margin: 20rpx;
	padding: 20rpx;
	margin-bottom: 220rpx;
}
.content-item {
	display: flex;
	justify-content: space-between;
}
.content-item-title {
	display: flex;
	align-items: center;
	justify-content: center;
}
.content-title {
	font-size: 40rpx;
	margin-bottom: 40rpx;
}
.content-item-part {
	width: 500rpx;
	text-align: right;
}
.cross-line {
	margin: 26rpx 0rpx;
	width: 100%;
	height: 2rpx;
	background: #F1F3F2;
}
.bottom {
	padding: 10rpx;
	padding-top: 14rpx;
	background: white;
	box-shadow: 0px -2rpx 2rpx #cacaca;
	position: fixed;
	bottom: 0;
	width: 100vw;
}
.bottom-tip {
	font-size: 26rpx;
}
.radio-style {
	-webkit-transform: scale(0.8);
	        transform: scale(0.8);
}
.button {
	margin: 20rpx;
	height: 70rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 34rpx;
	background: #00cb95;
	color: white;
}
.gray {
	margin-left: 10rpx;
	color: #939493;
}

