.goods-img {
	margin-top: 30rpx;
	margin-left: 30rpx;
	width: 120rpx;
	height: 120rpx;
	-webkit-box-flex: 2;
	-webkit-flex: 2;
	        flex: 2;
	border-radius: 40rpx;
}
