	width: 120rpx;
	height: 120rpx;
	border-radius: 40rpx;
	box-shadow: 0rpx 10rpx 20rpx #cdcecd;
}
.goods-detail {
	margin-top: 4rpx;
	margin-left: 0;
	-webkit-box-flex: 7;
	-webkit-flex: 7;
	        flex: 7;
}
.buttonSelected {
	background-color: #4ECF68 !important;
}
.release {
	background-color: #f3f2f2;
}
.repair-border {
	background-color: white;
	margin-top: 30rpx;
	margin-bottom: 30rpx;
}
.repair-item {
	
	display: -webkit-box;
	
	display: -webkit-flex;
	
	display: flex;
	padding: 20rpx 0rpx;
	margin: 0 30rpx;
	background-color: white;
}
.repair-text {
	-webkit-box-flex: 2;
	-webkit-flex: 2;
	        flex: 2;
	font-size: 32rpx !important;
	line-height: 60rpx;
	padding-left: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.release-category {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.category-text {		
	font-size: 32rpx !important;
	line-height: 60rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding-left: 30rpx;
	width: 100rpx;
}
.release-scroll {
	-webkit-box-flex: 6;
	-webkit-flex: 6;
	        flex: 6;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	line-height: 60rpx;
	white-space: nowrap;
	overflow: hidden;
	width: 400rpx;
}
.release-scroll view {
	display: inline-block;
	border-radius: 20rpx;
	background-color: #a6a6a6;
	padding: 2rpx 4rpx;
	margin:0 10rpx;
	color: white;
	margin-bottom: 5rpx;
}
.repair-input {
	-webkit-box-flex: 4;
	-webkit-flex: 4;
	        flex: 4;
}
.repair-input .uni-input {
	/* border: 0.5px solid #A6A7A6; */
	border-radius: 12rpx;
	padding: 10rpx;
	font-size: 32rpx;
	color: #A6A7A6;
	line-height: 40rpx;
	/* margin-right: 20rpx; */
}
.repair-input-image view {
	display: inline-block;
	margin-right: 30rpx;
	/* margin-bottom: 30rpx; */
}
.repair-image {
	height: 120rpx;
	width: 120rpx;
	border-radius: 30rpx !important;
}
